{
  "name": "react-mgr",
  "version": "0.0.1-rc21",
  "description": "React carousel component.",
  "keywords": [
    "react",
    "carousel",
    "merry-go-round",
    "component"
  ],
  "author": {
    "name": "Rostyslav Khanas",
