    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "react-scrolling": "^0.0.1-rc.32"
  },
  "peerDependencies": {
    "react": "~15.3.2"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
